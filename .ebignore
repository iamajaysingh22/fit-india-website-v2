# .ebignore - Files to exclude from Elastic Beanstalk deployment

# Local build output (will be built on server)
.next/
out/

# Node modules (will be installed on server)
node_modules/

# Development files
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Git files
.git/
.gitignore

# Testing
coverage/
.nyc_output

# Build tools
.turbo/

# Vercel
.vercel

# Debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Temporary folders
tmp/
temp/
